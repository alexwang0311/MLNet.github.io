<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RunAOTCompilation>true</RunAOTCompilation>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="7.0.11" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="7.0.11" PrivateAssets="all" />
    <PackageReference Include="MudBlazor" Version="6.11.0" />
  </ItemGroup>

  <ItemGroup>
    <NativeFileReference Include="C++\main.cpp" />
    <NativeFileReference Include="C++\ggml.c" />
    <NativeFileReference Include="C++\ggml-alloc.c" />
    <NativeFileReference Include="C++\ggml-backend.c" />
    <NativeFileReference Include="C++\common.cpp" />
    <NativeFileReference Include="C++\common-ggml.cpp" />
  </ItemGroup>

</Project>
